<!DOCTYPE mapper     PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="main.java.com.altarix.artifacttest2.mappers.CompanyMapper">
    <!--название метода--> <!--тип входящих параметров-->
    <select id="getByID" parameterType="long" >
    select * from subscriber where id = #{id}<!--поле в фигурных скобках это параметр, который прилетел в метод-->
